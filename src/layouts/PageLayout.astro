<style>
#wrapper {
  display: grid;
  grid-template-columns: 240px 1fr;
  grid-template-rows: auto 1fr;
  height: 100vh;
  grid-gap: 0;
  overflow: hidden;
}

#topbar {
  grid-column: 1 / -1;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: var(--plainframe-space-medium-900) var(--plainframe-space-medium-700) var(--plainframe-space-medium-900) var(--plainframe-space-medium-1300);
}

#navigation, #content {
  overflow-block: auto;
  scroll-behavior: smooth;
  overflow: scroll;
  scrollbar-color: var(--plainframe-color-primary-800) transparent;
  scrollbar-width: thin;
}

#logo {
  color: var(--plainframe-color-primary-1100);
}

#logo svg {
    display: block;
  height: var(--plainframe-size-large-700);
}

</style>
